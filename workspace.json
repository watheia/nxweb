{
  "version": 2,
  "projects": {
    "catalog": "libs/catalog",
    "env-gatsby-mui-emotion": "libs/env/gatsby/mui-emotion",
    "env-gatsby-site-schema": "libs/env/gatsby/site-schema",
    "env-gatsby-top-layout": "libs/env/gatsby/top-layout",
    "gatsby-theme-minimal": "apps/gatsby-theme-minimal",
    "theme-provider": "libs/theme/provider",
    "theme-styles-global": "libs/theme/styles/global",
    "ui-hooks-use-dark-mode": "libs/ui/hooks/use-dark-mode",
    "ui-hooks-use-local-storage": "libs/ui/hooks/use-local-storage",
    "ui-hooks-use-media": "libs/ui/hooks/use-media",
    "utils": "libs/utils",
    "www": {
      "root": "apps/www",
      "sourceRoot": "apps/www/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/gatsby:build",
          "options": {
            "outputPath": "apps/www/public",
            "uglify": true,
            "color": true,
            "profile": false
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/gatsby:server",
          "options": {
            "buildTarget": "www:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "www:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/www/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/www"],
          "options": {
            "jestConfig": "apps/www/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "www-e2e": {
      "root": "apps/www-e2e",
      "sourceRoot": "apps/www-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/www-e2e/cypress.json",
            "devServerTarget": "www:serve",
            "tsConfig": "apps/www-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/www-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "@emotion/styled"
      }
    }
  },
  "defaultProject": "www"
}
