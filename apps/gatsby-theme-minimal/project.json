{
  "root": "apps/gatsby-theme-minimal",
  "sourceRoot": "apps/gatsby-theme-minimal/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/gatsby:build",
      "options": {
        "outputPath": "apps/gatsby-theme-minimal/public",
        "uglify": true,
        "color": true,
        "profile": false
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/gatsby:server",
      "options": {
        "buildTarget": "gatsby-theme-minimal:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "gatsby-theme-minimal:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/gatsby-theme-minimal/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
