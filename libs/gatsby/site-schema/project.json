{
  "root": "libs/gatsby/site-schema",
  "sourceRoot": "libs/gatsby/site-schema/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/gatsby/site-schema/**/*.js"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/gatsby/site-schema"
      ],
      "options": {
        "jestConfig": "libs/gatsby/site-schema/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/gatsby/site-schema",
        "tsConfig": "libs/gatsby/site-schema/tsconfig.lib.json",
        "packageJson": "libs/gatsby/site-schema/package.json",
        "main": "libs/gatsby/site-schema/index.js",
        "assets": [
          "libs/gatsby/site-schema/*.md"
        ]
      }
    }
  },
  "tags": []
}
